{
  "name": "rafaell-lycan.github.io",
  "private": true,
  "version": "3.0.0",
  "description": "Rafaell Lycan's website",
  "homepage": "https://github.com/rafaell-lycan/rafaell-lycan.github.com#readme",
  "author": "Rafaell Lycan <sonny.webdsg@gmail.com>",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf _site",
    "dev": "npm run clean && concurrently \"npm run webpack:dev\" \"npm run jekyll:dev\"",
    "build": "npm run clean && npm run webpack && npm run jekyll",
    "jekyll": "bundle exec jekyll build",
    "jekyll:dev": "bundle exec jekyll serve --incremental",
    "webpack": "webpack --progress --config webpack.config.js",
    "webpack:dev": "webpack --progress --config webpack.config.js --colors --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.5.0",
    "rimraf": "^2.6.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafaell-lycan/rafaell-lycan.github.com.git"
  },
  "bugs": {
    "url": "https://github.com/rafaell-lycan/rafaell-lycan.github.com/issues"
  }
}
